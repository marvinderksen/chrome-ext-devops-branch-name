{
  "manifest_version": 3,
  "name": "Azure DevOps â€” Copy branch name",
  "description": "Copy branch names easily in Azure DevOps",
  "version": "2.0",
  "author": "marvinderksen",
  "homepage_url": "https://github.com/marvinderksen/chrome-ext-devops-branch-name",
  "action": {
    "default_popup": "about.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "js": ["inject.js"],
      "matches": ["https://dev.azure.com/*"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["inject.js", "button.js", "page-pr.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
